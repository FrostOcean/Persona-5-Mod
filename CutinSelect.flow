import( "CutinSelect.msg" );
//Cutin Test
void CutinTestDisplay()
{
    SetConfidantNames();
    while (true)
    {
        MSG_WND_DSP();
        int selection = SEL_GENERIC(PartyMemberCutinMenuName, PartyCharacterSelect);
        MSG_WND_CLS();
        switch ( selection ) 
        {
            case -1:
                return;
            case 0:
                CutinMenu( 2 );
                break;
            case 1:
                CutinMenu( 4 );
                break;
            case 2:
                CutinMenu( 6 );
                break;
            case 3:
                CutinMenu( 8 );
                break;    
            case 4:
                CutinMenu( 10 );
                break;
            case 5:
                CutinMenu( 12 );
                break;    
            case 6:
                CutinMenu( 14 );
                break;
            case 7:
                CutinMenu( 16 );
                break;    
            case 8:
                CutinMenu( 18 );
                break;    
        }
    }
}

void CutinMenu(int msgID)
{
    while (true)
    {
        int selection = SEL_GENERIC_NOT_HELP(CutinOrientationMenuName, CutinOrientation);
        DisplayMessagePrompt( msgID + selection );
    }
}
